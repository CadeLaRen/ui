<span id="<%= id %>"></span>
<script>
  $('#<%= id %>').ninjaui.buttonCreate({<%= "clicked:#{clicked}, " unless !defined? clicked -%>
<%= "disabled:#{disabled}, " unless !defined? disabled -%>
<%= "icon:'#{icon}', " unless !defined? icon -%>
<%= "onClick:#{onClick}, " unless !defined? onClick -%>
<%= "submittable:#{submittable}, " unless !defined? submittable -%>
<%= "title:'#{title}'" -%>});
</script>
